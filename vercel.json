{
  "version": 2,
  "packageManager":"npm@9.2.0",
  "buildCommand":"npm install typescript; npm run build; npm run prod",
  "devCommand":"npm run prod",

  "builds": [
    {
      "src": "package.json",
      "use": "vercel-sapper"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}