{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "installCommand": "npm i",
  "devCommand": "npm run start",
  "routes": [
    {
      "src": "./dist/(.*)",
      "dest": "./dist/index.js"
    }
  ],
  "rewrites": [{ "source": "./dist/(.*)", "destination": "./dist/index.js" }]
}
