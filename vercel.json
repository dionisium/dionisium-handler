{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node"}
  ],
  "routes": [
    { "src": "/dist/(.*)", "dest": "/dist/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run start"
}