{
  "builds": [
    { "src": "package.json", "use": "npm install typescript; npm run build" }
  ],
  "outputDirectory": "./dist",
  "routes": [
    { "src": "/dist/(.*)", "dest": "/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install",
  "devCommand": "npm run prod"
}